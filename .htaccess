
RewriteEngine On
    RewriteBase /
    RewriteRule ^api$    datcom/controller/api.php    [NC,L]

    #guid template = [a-zA-Z0-9]{8}-[a-zA-Z0-9]{4}-[a-zA-Z0-9]{4}-[a-zA-Z0-9]{4}-[a-zA-Z0-9]{12}
    RewriteRule ^group-management/([a-z0-9]+)/([a-zA-Z0-9]{8}-[a-zA-Z0-9]{4}-[a-zA-Z0-9]{4}-[a-zA-Z0-9]{4}-[a-zA-Z0-9]{12})$    datcom/view/group-management.php?groupCode=$1&hash=$2    [NC,L]
